<?xml version="1.0" encoding="UTF-8"?>
<config>
    
	<params>
        <param name="OPPO_AD_APP_ID" required="1" showName="AppID" desc="广告联盟上申请的appid" bWriteInManifest="0" bWriteInClient="1" />
        <param name="OPPO_AD_SPLASH_POS_ID" required="1" showName="闪屏广告位ID" desc="广告联盟上申请的闪屏广告的广告位ID" bWriteInManifest="0" bWriteInClient="1" />
        <param name="OPPO_AD_SPLASH_TITLE" required="1" showName="闪屏广告标题" desc="闪屏广告下方显示的标题" bWriteInManifest="0" bWriteInClient="1" />
        <param name="OPPO_AD_SPLASH_DESC" required="1" showName="闪屏广告描述" desc="闪屏广告下方显示的描述" bWriteInManifest="0" bWriteInClient="1" />
        <param name="OPPO_AD_BANNER_POS_ID" required="1" showName="Banner广告位ID" desc="广告联盟上申请的Banner广告的广告位ID" bWriteInManifest="0" bWriteInClient="1" />
        <param name="OPPO_AD_POPUP_POS_ID" required="1" showName="插屏广告位ID" desc="广告联盟上申请的插屏广告的广告位ID" bWriteInManifest="0" bWriteInClient="1" />
        <param name="OPPO_AD_VIDEO_POS_ID" required="1" showName="视频广告位ID" desc="广告联盟上申请的激励视频的广告位ID" bWriteInManifest="0" bWriteInClient="1" />
        <param name="OPPO_AD_NATIVE_POS_ID" required="1" showName="原生广告位ID" desc="广告联盟上申请的原生广告位ID(非模板)" bWriteInManifest="0" bWriteInClient="1" />
    </params>
    
    <operations>
        <operation step="1" type="mergeManifest" from="SDKManifest.xml" to="AndroidManifest.xml" />
        <operation step="2" type="copyRes" from="assets" to="assets" />
        <operation step="3" type="copyRes" from="libs" to="lib" />
        <operation step="4" type="copyRes" from="res" to="res" />
        <operation step="4" type="copyRes" from="root" to="" />
    </operations>
    
    <plugins>
        <plugin name="com.u8.sdk.ad.oppo.OppoAd" type="7" desc="广告接口"/>
    </plugins>
    
    <version>
        <name>Oppo广告联盟</name>
        <pluginType>ad</pluginType>
        <versionCode>1</versionCode>
        <versionName>3.1.3</versionName>
    </version>     
    
</config>