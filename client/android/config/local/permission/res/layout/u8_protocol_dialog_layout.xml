<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
android:background="@drawable/u8_dialog_corner_bg"
android:paddingTop="5dp"
android:orientation="vertical"
android:paddingBottom="5dp"
android:layout_width="fill_parent"
android:layout_height="wrap_content">

    <LinearLayout android:layout_width="fill_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp">
        <WebView android:layout_width="match_parent"
            android:id="@+id/u8_webview_protocol"
            android:layout_height="wrap_content">
        </WebView>
    </LinearLayout>


    <LinearLayout
        android:orientation="horizontal"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="10dp"
        android:layout_width="match_parent"
        android:layout_height="50dp">
        <Button
            android:id="@+id/u8_no_agree"
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:layout_weight="1"
            android:text="不同意"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="7dp"
            android:textSize="18sp"
            android:textColor="@color/u8_dark"
            android:background="@drawable/u8_corner_gray"
            android:gravity="center"/>


        <Button
            android:id="@+id/u8_agree"
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:layout_weight="1"
            android:textSize="18sp"
            android:layout_marginLeft="7dp"
            android:layout_marginRight="15dp"
            android:text="同意"
            android:textColor="@android:color/white"
            android:background="@drawable/u8_corner_button"
            android:gravity="center"/>

    </LinearLayout>


</LinearLayout>
